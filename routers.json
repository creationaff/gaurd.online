{
    "Netgear": {
        "steps": [
            "Log in to http://www.routerlogin.net",
            "Go to Advanced -> Setup -> Internet Setup",
            "Under 'Domain Name Server (DNS) Address', select 'Use These DNS Servers'",
            "Enter 1.1.1.3 and 1.0.0.3",
            "Click Apply"
        ],
        "default_ip": "192.168.1.1"
    },
    "TP-Link": {
        "steps": [
            "Log in to http://tplinkwifi.net",
            "Go to Advanced -> Network -> DHCP Server",
            "Set Primary DNS to 1.1.1.3 and Secondary DNS to 1.0.0.3",
            "Click Save"
        ],
        "default_ip": "192.168.0.1"
    },
    "ASUS": {
        "steps": [
            "Log in to http://router.asus.com",
            "Go to WAN -> Internet Connection",
            "Under 'WAN DNS Setting', set 'Connect to DNS Server automatically' to No",
            "Enter 1.1.1.3 and 1.0.0.3",
            "Click Apply"
        ],
        "default_ip": "192.168.1.1"
    }
}
